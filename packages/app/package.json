{
  "name": "@pma/app",
  "version": "1.0.0",
  "author": "Panenco <info@panenco.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.development.js",
    "start:development": "cross-env PROJECT_ENV=development npm run start",
    "start:staging": "cross-env PROJECT_ENV=staging npm run start",
    "start:production": "cross-env PROJECT_ENV=production npm run start",
    "build": "webpack --config config/webpack.config.production.js",
    "build:development": "cross-env BABEL_ENV=production PROJECT_ENV=development npm run build",
    "build:staging": "cross-env BABEL_ENV=production PROJECT_ENV=staging npm run build",
    "build:production": "cross-env BABEL_ENV=production PROJECT_ENV=production npm run build",
    "clean": "rimraf dist",
    "analyzer": "cross-env NODE_ENV=production BABEL_ENV=production PROJECT_ENV=production webpack --config config/webpack.config.production.js --analyze",
    "lint:js": "eslint config src --ext .js --ext .jsx",
    "lint:css": "stylelint 'src/**/*.scss'",
    "prettier": "prettier '{config,src}/**/*.{js,jsx}' --write",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js --fix --"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@loadable/component": "^5.13.1",
    "@pma/ui": "^1.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-json-strings": "7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-react-constant-elements": "^7.10.4",
    "@babel/plugin-transform-react-inline-elements": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@hot-loader/react-dom": "^16.13.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "cypress": "^5.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-cypress": "^2.11.1",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-order": "^4.1.0",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.0",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
